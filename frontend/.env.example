# Frontend Environment Variables
# Copy this file to .env.local and fill in the actual values

# API Configuration
VITE_API_URL=http://localhost:8000/api/v1
VITE_API_TIMEOUT=30000

# Keycloak Configuration (if using Keycloak authentication)
VITE_KEYCLOAK_URL=http://localhost:8080
VITE_KEYCLOAK_REALM=sistema-laudos
VITE_KEYCLOAK_CLIENT_ID=sistema-laudos

# App Configuration
VITE_APP_NAME=Sistema de Laudos
VITE_APP_VERSION=1.0.0
VITE_LOG_LEVEL=error

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
