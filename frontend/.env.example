# Frontend Environment Variables
# Copy this file to .env.dev/.env.prod and fill in the actual values

# API Configuration
VITE_API_URL=http://localhost:8000/api

# Keycloak OIDC Configuration
VITE_KEYCLOAK_URL=http://localhost:8081
VITE_KEYCLOAK_REALM=sistema_laudos_dev
VITE_KEYCLOAK_CLIENT_ID=sistema_laudos_backend_dev
VITE_KEYCLOAK_CLIENT_SECRET=your-client-secret-here

# App Configuration
VITE_APP_NAME=Sistema de Laudos
VITE_APP_VERSION=1.0.0
VITE_LOG_LEVEL=error

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
